<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<navigation-rule>
		<display-name>viewCities</display-name>
		<from-view-id>/city/createCity.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>created</from-outcome>
			<to-view-id>/city/viewCities.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<application>
	    <resource-bundle>
	        <base-name>com.freecode.xproject.messages</base-name>
	        <var>msgs</var>
  	    </resource-bundle>
        <locale-config>
            <default-time-zone-id>America/Los_Angeles</default-time-zone-id>
        </locale-config>
	</application>
</faces-config>